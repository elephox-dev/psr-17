{
    "name": "elephox/psr-17",
    "description": "The PSR-17 adapter for Elephox.",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "elephox/http": "dev-main",
        "elephox/psr-7": "dev-main",
        "elephox/stream": "dev-main",
        "jetbrains/phpstorm-attributes": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "provide": {
        "psr/http-factory-implementation": "1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Elephox\\Psr17\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ricardo Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "http-interop/http-factory-tests": "^0.9.0",
        "phpunit/phpunit": "^9.5"
    }
}
